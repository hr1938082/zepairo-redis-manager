{
    "compilerOptions": {
        "target": "ES2021",
        "module": "NodeNext",
        "moduleResolution": "nodenext",
        "strict": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "declaration": true,
        "outDir": "./dist",
        "resolveJsonModule": true,
        "lib": [
            "ES2021"
        ]
    },
    "exclude": [
        "dist",
        "node_modules",
        "src/config/redis.ts"
    ]
}